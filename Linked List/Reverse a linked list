 class Solution {
  public:
    Node* reverseList(struct Node* head) {
         Node* curr=head;
       Node* prev=NULL;
        Node* temp=NULL;
        while(curr!=NULL){
            temp=curr->next;
            curr->next=prev;
            prev=curr;
            curr=temp;
            
        }
        return prev;
        
    }
};
