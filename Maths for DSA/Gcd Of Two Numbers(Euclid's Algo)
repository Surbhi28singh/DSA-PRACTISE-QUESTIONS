#include <iostream>
using namespace std;
//function for gcd 
int gcd(int a,int b){
    if(a==0){
        return a;
    }
    if(b==0){
        return b;
    }
    while(a!=b){
        if(a>b){
            a=a-b;
        }
        else{
            b=b-a;
        }
    }
    
        return a;
    }

int main()
{
    cout<<"enter value of a and b "<<endl;
    int a;
    int b;
    cin>>a>>b;
    int ans=gcd(a,b);
    cout<<"gcd of a and b is"<<ans<<endl;
}
